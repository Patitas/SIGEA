<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <constant name="struts devMode" value="true"/>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="consultaMate" class="edu.utez.Acciones.AccionesMaterias" method="consultaMaterias">
            <result name="success">/Jsp/JspAsignarHorarios.jsp</result>
        </action>
        
        <action name="consultaProfe" class="edu.utez.Acciones.AccionesProfesor" method="consultarProfesor">
            <result name="success">/Jsp/JspAsignarHorarios.jsp</result>
        </action>
        
        <action name="consultarGrupos" class="sigea.mx.Acciones.AccionesGrupo" method="consultarGrupos">
            <result name="success">/Jsp/JspAltaGrupos.jsp</result>
        </action>
        
    </package>    
</struts>
